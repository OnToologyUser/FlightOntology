#################################################################

###  http://www.semanticweb.org/borja/ontologies/2024/3/FlightSatisfaction#Owns
:Owns rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      rdfs:domain :Airline ;
      rdfs:range :Aircraft .


###  http://www.semanticweb.org/borja/ontologies/2024/3/FlightSatisfaction#Writes
:Writes rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:domain :Traveller ;
        rdfs:range :Review .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/borja/ontologies/2024/3/FlightSatisfaction#hasAircraft
:hasAircraft rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Airline ;
             rdfs:range xsd:int .


###  http://www.semanticweb.org/borja/ontologies/2024/3/FlightSatisfaction#hasReviews
:hasReviews rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Traveller ;
            rdfs:range xsd:int .


###  http://www.semanticweb.org/borja/ontologies/2024/3/FlightSatisfaction#hasValue
:hasValue rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:integer .


###  http://www.semanticweb.org/borja/ontologies/2024/3/FlightSatisfaction#nCompanions
:nCompanions rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Traveller ;
             rdfs:range xsd:integer .


